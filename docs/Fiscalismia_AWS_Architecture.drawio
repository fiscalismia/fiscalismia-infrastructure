        <mxCell id="xqXAjq9A-HITpdVLmNYo-191" value="API Gateway Autoscaling DDos/Cost Protection" style="rounded=0;whiteSpace=wrap;html=1;fontStyle=0;fontSize=16;align=center;fillColor=#d5e8d4;strokeColor=#82b366;fontFamily=Helvetica;labelBorderColor=none;snapToPoint=0;fixDash=0;" parent="1" vertex="1">
